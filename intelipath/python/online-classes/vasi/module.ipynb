    "    total = 0\n",
    "    for num in nums:\n",
    "        total += num\n",
    "    return total\n",
    "\n",
    "def sub(*nums):\n",
    "    total = 0\n",
    "    if(nums): total = nums[0]\n",
    "    for i in range(1, len(nums)):\n",
    "        total -= nums[i]\n",
    "    return total"
